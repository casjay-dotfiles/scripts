# Import config
[ -f "$MYLOCATION_CONFIG_DIR/$MYLOCATION_CONFIG_FILE" ] && . "$MYLOCATION_CONFIG_DIR/$MYLOCATION_CONFIG_FILE"
# - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
# Ensure Directories exist
[ -d "$MYLOCATION_LOG_DIR" ] || mkdir -p "$MYLOCATION_LOG_DIR" |& __devnull
[ -d "$MYLOCATION_TEMP_DIR" ] || mkdir -p "$MYLOCATION_TEMP_DIR" |& __devnull
[ -d "$MYLOCATION_CACHE_DIR" ] || mkdir -p "$MYLOCATION_CACHE_DIR" |& __devnull
# - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
MYLOCATION_TEMP_FILE="${MYLOCATION_TEMP_FILE:-$(mktemp $MYLOCATION_TEMP_DIR/XXXXXX 2>/dev/null)}"
# - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
