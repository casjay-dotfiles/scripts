    else
      printf_exit "Failed to update $ASDF_DIR" >&2
    fi
  else
    printf_green "Downloading asdf from $GITREPO"
    if __git_clone_repo "$GITREPO" "$ASDF_DIR"; then
      printf_blue "Downloaded asdf to $ASDF_DIR"
