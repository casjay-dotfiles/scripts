__setup_devbox() {
  local exitCode=0
  local name="devbox"
  if __cmd_exists $name; then
    printf_green "Attempting to update $name"
  else
    printf_green "Attempting to install $name"
  fi
  printf_green "Installing $name"
  if __curl "https://get.jetify.com/devbox" | bash -s -- -f |& __output_status; then
