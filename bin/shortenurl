# - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
__initialize_app() {
  [ -d "$SHORTENURL_CONFIG_DIR/urls" ] || mkdir -p "$SHORTENURL_CONFIG_DIR/urls"
  local load_urls=""
  load_urls="$(find "$SHORTENURL_CONFIG_DIR/urls/" -type f -name '*.sh' 2>/dev/null | wc -l)"
  if [ "$load_urls" -ne 0 ]; then
    for url in "$SHORTENURL_CONFIG_DIR/urls"/*; do
      . "$url"
    done
  fi
}
