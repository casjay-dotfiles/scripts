# Execute commands

# - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
# begin main app
case "$1" in
github)
  shift 1
  if [ ${#} -eq 0 ]; then
    if [ $# -eq 0 ] && [ -p "/dev/stdin" ]; then
      seturl="$(</dev/stdin)"
    fi
  else
    seturl="$1"
  fi
  __github_url $seturl ${2:-$1}
  __output_results "$result"
  exitCode=$?
  ;;

private)
  shift 1
  if [ ${#} -eq 0 ]; then
    if [ $# -eq 0 ] && [ -p "/dev/stdin" ]; then
      seturl="$(</dev/stdin)"
    fi
  else
    seturl="$1"
  fi
  __private_url $seturl ${2:-$1}
  __output_results "$result"
  exitCode=$?
  ;;

is_gd)
  shift 1
  if [ ${#} -eq 0 ]; then
    if [ $# -eq 0 ] && [ -p "/dev/stdin" ]; then
      seturl="$(</dev/stdin)"
    fi
  else
    seturl="$1"
  fi
  __is_gd_url $seturl ${2:-$1}
  __output_results "$result"
  exitCode=$?
  ;;

tny_im)
  shift 1
  if [ ${#} -eq 0 ]; then
    if [ $# -eq 0 ] && [ -p "/dev/stdin" ]; then
      seturl="$(</dev/stdin)"
    fi
  else
    seturl="$1"
  fi
  __tny_url $seturl ${2:-$1}
  __output_results "$result"
  exitCode=$?
  ;;

yourls)
  shift 1
  if [ ${#} -eq 0 ]; then
    if [ $# -eq 0 ] && [ -p "/dev/stdin" ]; then
      seturl="$(</dev/stdin)"
    fi
  else
    seturl="$1"
  fi
  __yourls_url $seturl ${2:-$1}
  __output_results "$result"
  exitCode=$?
  ;;

"$1")
  if type __${1}_url 2>&1 | grep -q 'is a function' &>/dev/null; then
    name="$1"
    shift 1
    if [ ${#} -eq 0 ]; then
      if [ $# -eq 0 ] && [ -p "/dev/stdin" ]; then
        seturl="$(</dev/stdin)"
      fi
    else
      seturl="$1"
    fi
    __${name}_url $seturl ${2:-$1}
    __output_results "$result"
    exitCode=$?
  else
    if [ ${#} -eq 0 ]; then
      if [ $# -eq 0 ] && [ -p "/dev/stdin" ]; then
        seturl="$(</dev/stdin)"
      fi
    else
      seturl="$1"
    fi
    __private_url $seturl ${2:-$1}
    __output_results "$result"
    exitCode=$?
  fi
  ;;
esac
# - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
# Set exit code
exitCode="${exitCode:-0}"
# - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
# End application
# - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
# lets exit with code
exit ${exitCode:-0}
