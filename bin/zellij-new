# Color printf functions
printf_blue() { printf_color "$1\n" 4; }
printf_cyan() { printf_color "$1\n" 6; }
printf_green() { printf_color "$1\n" 2; }
printf_yellow() { printf_color "$1\n" 3; }
printf_red() { printf_color "$1\n" 1; }
printf_purple() { printf_color "$1\n" 5; }
printf_newline() { printf '\n'; }
printf_readline() { printf '%s\n' "$(cat -)"; }
printf_exit() { printf_red "$1" && exit ${2:-1}; }
# - - - - - - - - - - - - - - - - - - - - - - - -
# Command and password helpers
cmd_exists() {
  local error_mode="" ask_mode="" exitCode=0
  while [ $# -gt 0 ]; do
    case "$1" in
    --error) error_mode="true" && shift 1 ;;
    --ask) ask_mode="true" && shift 1 ;;
    *) break ;;
    esac
  done
  for cmd in "$@"; do
    if ! builtin command -v "$cmd" &>/dev/null; then
      [ "$error_mode" = "true" ] && printf_red "$cmd is not installed"
      exitCode=1
    fi
  done
  return $exitCode
}
ask_for_password() {
  local cmd="${1:-sudo}"
  shift 1
  if [ "$cmd" = "sudo" ]; then
    sudo -vn 2>/dev/null || sudo -v
    return $?
  else
    return 0
  fi
}
