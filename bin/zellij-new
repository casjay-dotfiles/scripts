# - - - - - - - - - - - - - - - - - - - - - - - - -
# Cache helper function
__cache_valid() {
  local cache_file="$1"
  local cache_ttl="$2"
  local current_time cache_time time_diff
  if [ -f "$cache_file" ]; then
    current_time=$(date +%s)
    cache_time=$(stat -c %Y "$cache_file" 2>/dev/null || stat -f %m "$cache_file" 2>/dev/null || echo 0)
    time_diff=$((current_time - cache_time))
    if [ "$time_diff" -lt "$cache_ttl" ]; then
      return 0
    fi
  fi
  return 1
}
# - - - - - - - - - - - - - - - - - - - - - - - - -
# Weather function with caching
__exec_weather() {
  local cache_dir="${ZELLIJ_CACHE_DIR:-$HOME/.cache/zellij}"
  local cache_file="$cache_dir/weather-$$"
  local cache_ttl="${ZELLIJ_WEATHER_INTERVAL:-3600}"
  local weather_server="${ZELLIJ_WEATHER_SERVER:-https://wthr.top}"
  local weather_location="${MYLOCATION_NAME:-${ZELLIJ_WEATHER_LOCATION:-New York, NY}}"
  local weather_url
  weather_url=$(echo "${weather_location}" | sed 's/ /%20/g')
  [ -d "$cache_dir" ] || mkdir -p "$cache_dir" 2>/dev/null
  if __cache_valid "$cache_file" "$cache_ttl"; then
    cat "$cache_file" 2>/dev/null || echo "No Weather"
  else
    local weather
    weather=$(curl -q -LSsf -m 2 "${weather_server}/${weather_url}?u&format=3" 2>/dev/null | sed 's/:/ /g;s/Â°F/F/g' || echo "No Weather")
    echo "$weather" >"$cache_file" 2>/dev/null
    echo "$weather"
  fi
}
# - - - - - - - - - - - - - - - - - - - - - - - - -
# Uptime function with caching
__exec_uptime() {
  local cache_dir="${ZELLIJ_CACHE_DIR:-$HOME/.cache/zellij}"
  local cache_file="$cache_dir/uptime-$$"
  local cache_ttl="${ZELLIJ_UPTIME_INTERVAL:-60}"
  [ -d "$cache_dir" ] || mkdir -p "$cache_dir" 2>/dev/null
  if __cache_valid "$cache_file" "$cache_ttl"; then
    cat "$cache_file" 2>/dev/null
  else
    local uptime_info
    uptime_info=$(uptime | cut -f 4-5 -d ' ' | cut -f 1 -d ',' 2>/dev/null)
    echo "$uptime_info" >"$cache_file" 2>/dev/null
    echo "$uptime_info"
  fi
}
# - - - - - - - - - - - - - - - - - - - - - - - - -
__init() {
  local name="$1"
  name="${name##*/}"
  if [ -f "$ZELLIJ_NEW_FILE_CONF/$name" ]; then
    ZELLIJ_NEW_NAME="$name"
  elif [ -f "$ZELLIJ_NEW_FILE_CONF/$name-$name" ]; then
    ZELLIJ_NEW_NAME="$name-$name"
  elif [ -f "$ZELLIJ_NEW_FILE_CONF/$USER-$name" ]; then
    ZELLIJ_NEW_NAME="$USER-$name"
  elif [ -f "$ZELLIJ_NEW_FILE_CONF/$name-$USER" ]; then
    ZELLIJ_NEW_NAME="$name-$USER"
  else
    ZELLIJ_NEW_NAME="$name"
  fi
}
# - - - - - - - - - - - - - - - - - - - - - - - - -
__zellij_kill_all() {
  local exitCode=0
  local session_info=""
  local session_name=""
  local session_path=""
  local session_type=""

  printf_cyan "Capturing session information..."

  # Get all sessions with their paths
  local sessions="$(zellij list-sessions -F '#{session_name}:#{session_path}' 2>/dev/null)"

  if [ -z "$sessions" ]; then
    printf_yellow "No active sessions to kill"
    return 0
  fi

  # Kill all sessions and capture their info
  while IFS=: read -r session_name session_path; do
    if [ -n "$session_name" ]; then
      printf_cyan "Killing session: $session_name (path: $session_path)"
      zellij delete-session "$session_name" &>/dev/null

      # Clean up resurrect data
      local resurrect_dir="${HOME}/.local/share/zellij/resurrect/${session_name}"
      if [ -d "$resurrect_dir" ]; then
        rm -rf "$resurrect_dir"
      fi

      # Extract session type from name (e.g., "single-scripts" -> "single")
      if [[ "$session_name" =~ ^([a-z]+)- ]]; then
        session_type="${BASH_REMATCH[1]}"
      else
        session_type="single"
      fi

      # Store session info for recreation
      session_info+="${session_type}:${session_path}\n"
    fi
  done <<<"$sessions"

  printf_green "All sessions killed and cleaned"

  # Recreate sessions
  if [ -n "$session_info" ]; then
    printf_cyan "Recreating sessions in background..."
    echo -e "$session_info" | while IFS=: read -r session_type session_path; do
      if [ -n "$session_path" ] && [ -d "$session_path" ]; then
        printf_blue "Recreating $session_type session in: $session_path"
        (cd "$session_path" && "$0" --dir "$session_path" --silent "$session_type" &>/dev/null &)
      fi
    done
    sleep 2
    printf_green "Sessions recreated - use 'zellij-new list' to see them"
  fi

  return $exitCode
}
# - - - - - - - - - - - - - - - - - - - - - - - - -
__zellij_kill() {
  local exitCode=0
  local session="$(zellij list-sessions 2>/dev/null)"
  case "$1" in
  all)
    shift 1
    __zellij_kill_all
    return $?
    ;;
  session)
    shift
    [ $# -ne 0 ] || printf_cyan "Usage: $APPNAME kill session [name]"
    for arg in "$@"; do
      local retVal=0
      exists="$(echo "$session" | grep "^$arg" || echo '')"
      if [ -n "$exists" ]; then
        printf_cyan "Killing session $arg"
        zellij delete-session "$arg" &>/dev/null
        retval=$?
        exitCode=$((exitCode + retval))
      else
        printf_yellow "The session $arg does not exist"
      fi
    done
    return $exitCode
    ;;
  *)
    printf_cyan "Usage: $APPNAME kill [all|session] [name]"
    printf_cyan "Note: Zellij does not support killing individual tabs/panes from CLI"
    if [ -n "$session" ]; then
      printf '%s\n' "$session"
    fi
    exitCode=$?
    return $exitCode
    ;;
  esac
}
# - - - - - - - - - - - - - - - - - - - - - - - - -
__cd() {
  ZELLIJ_NEW_CWD="${1:-$ZELLIJ_NEW_CWD}"
  mkdir -p "$ZELLIJ_NEW_CWD" 2>/dev/null
  builtin cd "$ZELLIJ_NEW_CWD" || return 1
}
# - - - - - - - - - - - - - - - - - - - - - - - - -
__zellij_get_session_sockets() {
  [ -d "$ZELLIJ_NEW_SOCKET_DIR" ] || return 1
  local socket_sessions=""
  for socket in "$ZELLIJ_NEW_SOCKET_DIR"/*; do
    [ -e "$socket" ] || continue
    [ "$(basename "$socket")" = "default" ] && continue
    socket_sessions+="$(zellij -S "$socket" ls 2>/dev/null)"$'\n'
  done
  [ -n "$socket_sessions" ] && printf '%s' "$socket_sessions" | awk -F ':' '{print $1}' || return 1
}
# - - - - - - - - - - - - - - - - - - - - - - - - -
__zellij_get_all_sessions() {
  local all_sessions=""
  all_sessions="$(zellij list-sessions 2>/dev/null | grep -v '^$' || true)"
  if [ -n "$all_sessions" ]; then
    printf '%s\n' "$all_sessions" | awk '{print $1}'
  else
    return 1
  fi
}
# - - - - - - - - - - - - - - - - - - - - - - - - -
__zellij_list() {
  __zellij_get_all_sessions 2>/dev/null
}
# - - - - - - - - - - - - - - - - - - - - - - - - -
__create_zellij_conf() {
  [ -n "$1" ] && [ -n "$ZELLIJ_NEW_NAME" ] || ZELLIJ_NEW_NAME="${1:-${ZELLIJ_NEW_NAME:-default}}"
  mkdir -p "$ZELLIJ_NEW_CONFIG_DIR" 2>/dev/null
  if [ ! -f "$ZELLIJ_NEW_CONFIG_DIR/template.kdl" ]; then
    if [ -f "$CASJAYSDEVDIR/templates/zellij/zellij-new.kdl" ]; then
      cp -Rf "$CASJAYSDEVDIR/templates/zellij/zellij-new.kdl" "$ZELLIJ_NEW_CONFIG_DIR/template.kdl" 2>/dev/null
    else
      touch "$ZELLIJ_NEW_CONFIG_DIR/template.kdl" 2>/dev/null
    fi
  fi
  [ -f "$ZELLIJ_NEW_CONFIG_DIR/template.kdl" ]
}
# - - - - - - - - - - - - - - - - - - - - - - - - -
__launch_zellij() {
  [ -n "$1" ] && [ -n "$ZELLIJ_NEW_NAME" ] || ZELLIJ_NEW_NAME="${1:-${ZELLIJ_NEW_NAME:-default}}"
  [ -f "$ZELLIJ_NEW_FILE_LAYOUTS/$ZELLIJ_NEW_NAME.kdl" ] || printf_exit "No layout file found for $ZELLIJ_NEW_NAME"
  if [ -z "$ZELLIJ" ]; then
    __cd
    printf_cyan "Starting new zellij session for $ZELLIJ_NEW_NAME"
    zellij --layout "$ZELLIJ_NEW_FILE_LAYOUTS/$ZELLIJ_NEW_NAME.kdl" --session "$ZELLIJ_NEW_NAME" &>"$ZELLIJ_NEW_LOG_DIR/$APPNAME.log"
    return $?
  else
    printf_exit "You appear to already have zellij running"
  fi
}
# - - - - - - - - - - - - - - - - - - - - - - - - -
__zellij_conf() {
  [ -n "$1" ] && [ -n "$ZELLIJ_NEW_NAME" ] || ZELLIJ_NEW_NAME="${1:-${ZELLIJ_NEW_NAME:-default}}"
  if [ -f "$ZELLIJ_NEW_FILE_LAYOUTS/$ZELLIJ_NEW_NAME.kdl" ]; then
    return 0
  fi
  if [ ! -f "$ZELLIJ_NEW_FILE_LAYOUTS/$ZELLIJ_NEW_NAME.kdl" ]; then
    if [ -f "$ZELLIJ_NEW_CONFIG_DIR/template.kdl" ]; then
      printf_cyan "Creating $ZELLIJ_NEW_FILE_LAYOUTS/$ZELLIJ_NEW_NAME.kdl"
      cp -Rf "$ZELLIJ_NEW_CONFIG_DIR/template.kdl" "$ZELLIJ_NEW_FILE_LAYOUTS/$ZELLIJ_NEW_NAME.kdl"
    else
      printf_exit "Can not find $ZELLIJ_NEW_CONFIG_DIR/template.kdl"
    fi
  fi
  [ -f "$ZELLIJ_NEW_FILE_LAYOUTS/$ZELLIJ_NEW_NAME.kdl" ] && printf_blue "layout file has been created" || printf_exit "Failed to create layout file"
  return $?
}
# - - - - - - - - - - - - - - - - - - - - - - - - -
# Create zellij layout (tabs and panes) in KDL format
__create_windows() {
  if [ ! -f "$ZELLIJ_NEW_FILE_LAYOUTS/$ZELLIJ_NEW_NAME.kdl" ]; then
    local wname="" wcmd="" add_tabs="" wnum="0" && shift 1
    if [ $# -ne 0 ]; then
      for window in "$@"; do
        wnum=$(($wnum + 1))
        wname="$(echo "$window" | grep ':' | awk -F':' '{print $1}' | grep '^' || printf '%s' "${window:-tab$wnum}")"
        wcmd="$(echo "$window" | grep ':' | awk -F':' '{print $2}' | grep '^' || printf '%s' "${SHELL:-bash}")"
        add_tabs+="    tab name=\"$wname\" {
        pane command=\"$wcmd\"
    }
"
      done
    fi
    printf_green "Creating zellij layout file"
    cat <<EOF >"$ZELLIJ_NEW_FILE_LAYOUTS/$ZELLIJ_NEW_NAME.kdl"
// Layout config for $ZELLIJ_NEW_NAME
// Generated by zellij-new

layout {
$add_tabs}
EOF
    [ -f "$ZELLIJ_NEW_FILE_LAYOUTS/$ZELLIJ_NEW_NAME.kdl" ] && printf_blue "zellij layout file has been created" || printf_exit "Failed to create zellij layout file"
  fi
  return
}
# - - - - - - - - - - - - - - - - - - - - - - - - -
__edit() {
  [ -n "$EDITOR" ] || printf_exit "EDITOR is not set"
  [ -n "$1" ] && [ -n "$ZELLIJ_NEW_NAME" ] || ZELLIJ_NEW_NAME="${1:-${ZELLIJ_NEW_NAME:-default}}"
  [ -f "$ZELLIJ_NEW_FILE_LAYOUTS/$ZELLIJ_NEW_NAME.kdl" ] || printf_exit "Missing: $ZELLIJ_NEW_FILE_LAYOUTS/$ZELLIJ_NEW_NAME.kdl"
  eval $EDITOR "$ZELLIJ_NEW_FILE_LAYOUTS/$ZELLIJ_NEW_NAME.kdl"
  return $?
}
# - - - - - - - - - - - - - - - - - - - - - - - - -
__update_template() {
  if [ -f "$CASJAYSDEVDIR/templates/zellij/zellij-new.kdl" ]; then
    printf_cyan "Updating template from source..."
    cp -f "$CASJAYSDEVDIR/templates/zellij/zellij-new.kdl" "$ZELLIJ_NEW_CONFIG_DIR/template.kdl"
    if [ -f "$ZELLIJ_NEW_CONFIG_DIR/template.kdl" ]; then
      printf_green "Template updated successfully"
      return 0
    else
      printf_red "Failed to update template"
      return 1
    fi
  else
    printf_red "Source template not found: $CASJAYSDEVDIR/templates/zellij/zellij-new.kdl"
    return 1
  fi
}
# - - - - - - - - - - - - - - - - - - - - - - - - -
__update_all_configs() {
  local updated=0
  local failed=0

  if [ ! -f "$ZELLIJ_NEW_CONFIG_DIR/template.kdl" ]; then
    printf_red "Template not found. Run 'zellij-new update template' first"
    return 1
  fi

  printf_cyan "Updating all session layouts from template..."

  if [ -d "$ZELLIJ_NEW_FILE_LAYOUTS" ]; then
    for layout_file in "$ZELLIJ_NEW_FILE_LAYOUTS"/*.kdl; do
      [ -f "$layout_file" ] || continue
      local session_name="$(basename "$layout_file" .kdl)"
      printf_blue "Updating layout for session: $session_name"
      if cp -f "$ZELLIJ_NEW_CONFIG_DIR/template.kdl" "$layout_file" 2>/dev/null; then
        printf_green "âœ“ Updated: $session_name"
        updated=$((updated + 1))
      else
        printf_red "âœ— Failed: $session_name"
        failed=$((failed + 1))
      fi
    done
  fi

  printf_cyan "Update complete: $updated updated, $failed failed"

  if [ $failed -eq 0 ]; then
    return 0
  else
    return 1
  fi
}
# - - - - - - - - - - - - - - - - - - - - - - - - -
__zellij_attach() {
  local name="${1:-$(__zellij_list | tail -n1)}"
  [ -n "$name" ] || return 1
  zellij attach -t "$name" 2>/dev/null
  return $?
}
# - - - - - - - - - - - - - - - - - - - - - - - - -
__zellij_ssh() {
  local ssh_session="$(__zellij_get_all_sessions | grep '^ssh$')"
  [ -n "$ssh_session" ] && zellij -L ssh a -t ssh || return 1
}
# - - - - - - - - - - - - - - - - - - - - - - - - -
# Session management functions
__zellij_switch() {
  local session="${1:-$(__zellij_list | fzf 2>/dev/null || __zellij_list | head -1)}"
  [ -n "$session" ] || {
    printf_red "No session specified or available"
    return 1
  }

  if zellij has-session -t "$session" 2>/dev/null; then
    [ -n "$ZELLIJ" ] && zellij switch-client -t "$session" || zellij attach -t "$session"
    return $?
  else
    printf_red "Session '$session' does not exist"
    return 1
  fi
}
# - - - - - - - - - - - - - - - - - - - - - - - - -
__zellij_rename() {
  local old_name="${1}"
  local new_name="${2}"
  [ -n "$old_name" ] && [ -n "$new_name" ] || {
    printf_red "Usage: zellij-new rename <old_name> <new_name>"
    return 1
  }

  if zellij has-session -t "$old_name" 2>/dev/null; then
    zellij rename-session -t "$old_name" "$new_name"
    printf_green "âœ… Session '$old_name' renamed to '$new_name'"
    return $?
  else
    printf_red "Session '$old_name' does not exist"
    return 1
  fi
}
# - - - - - - - - - - - - - - - - - - - - - - - - -
__zellij_status() {
  local sessions="$(__zellij_list)"
  if [ -n "$sessions" ]; then
    printf_blue "ðŸ“Š Active zellij sessions:"
    for session in $sessions; do
      local info="$(zellij list-sessions -F '#{session_name}: #{session_windows} windows, #{session_attached} clients' 2>/dev/null | grep "^$session:")"
      [ -n "$info" ] && printf_green "  $info" || printf_yellow "  $session: (info unavailable)"
    done
  else
    printf_yellow "No active zellij sessions"
    return 1
  fi
}
# - - - - - - - - - - - - - - - - - - - - - - - - -
__zellij_clone() {
  local source="$1"
  local target="$2"
  [ -n "$source" ] && [ -n "$target" ] || {
    printf_red "Usage: zellij-new clone <source> <target>"
    return 1
  }

  if ! zellij has-session -t "$source" 2>/dev/null; then
    printf_red "Source session '$source' does not exist"
    return 1
  fi

  if zellij has-session -t "$target" 2>/dev/null; then
    printf_red "Target session '$target' already exists"
    return 1
  fi

  # Copy config files if they exist
  if [ -f "$ZELLIJ_NEW_FILE_CONF/$source" ] && [ -f "$ZELLIJ_NEW_FILE_WINDOWS/$source" ]; then
    cp "$ZELLIJ_NEW_FILE_CONF/$source" "$ZELLIJ_NEW_FILE_CONF/$target"
    cp "$ZELLIJ_NEW_FILE_WINDOWS/$source" "$ZELLIJ_NEW_FILE_WINDOWS/$target"
    __launch_zellij "$target"
    printf_green "âœ… Session '$source' cloned to '$target'"
  else
    printf_red "Cannot clone - config files not found for '$source'"
    return 1
  fi
}
# - - - - - - - - - - - - - - - - - - - - - - - - -
# Nested zellij with single status bar
__zellij_nested() {
  local session_name="${1:-nested-$(basename -- "$ZELLIJ_NEW_CWD")}"
  local parent_session="${2:-main}"

  # Create nested zellij config with disabled outer status bar
  local nested_config="$ZELLIJ_NEW_CONFIG_DIR/nested.conf"
  cat >"$nested_config" <<EOF
# Nested zellij configuration - single status bar
# Based on template but optimized for nesting

# Use different prefix for nested session
set-option -g prefix C-a
bind-key C-a send-prefix

# Disable status bar for outer session when nested
if-shell 'test -n "\$ZELLIJ"' 'set -g status off' 'set -g status on'

# Status bar configuration (only for inner/active session)
set -g status on
set -g status-position bottom
set -g status-justify left
set -g status-left-length 50
set -g status-right-length 150

# Visual indicators for nested session
set -g status-left '#[fg=green,bold][NESTED: #S] '
set -g status-right '#[fg=blue]#(whoami)@#h #[fg=yellow]%Y-%m-%d %H:%M'

# Window status format
set -g window-status-format '#[fg=white]#I:#W'
set -g window-status-current-format '#[fg=black,bg=green] #I:#W '

# Colors for nested session distinction
set -g status-bg black
set -g status-fg white

# Pane borders
set -g pane-border-style fg=blue
set -g pane-active-border-style fg=green

# Copy mode
setw -g mode-keys vi

# Mouse support with natural scrolling
set -g mouse on

# Natural mouse scrolling for nested sessions
bind-key -n WheelUpPane if-shell -F -t = "#{alternate_on}" "send-keys -M" "select-pane -t =; copy-mode -e; send-keys -M"
bind-key -n WheelDownPane select-pane -t = \\; send-keys -M

# In copy mode, scroll smoothly
bind-key -T copy-mode WheelUpPane send-keys -X -N 3 scroll-up
bind-key -T copy-mode WheelDownPane send-keys -X -N 3 scroll-down
bind-key -T copy-mode-vi WheelUpPane send-keys -X -N 3 scroll-up
bind-key -T copy-mode-vi WheelDownPane send-keys -X -N 3 scroll-down

# Disable mouse drag to prevent accidental selections
unbind-key -n MouseDrag1Pane
EOF

  if [ -n "$ZELLIJ" ]; then
    # We're inside zellij - create nested session
    printf_blue "ðŸ”— Creating nested zellij session: $session_name"

    # Disable status bar on current (outer) session
    zellij set -g status off

    # Launch nested session with new config
    zellij new-session -d -s "$session_name" -c "$ZELLIJ_NEW_CWD"
    zellij send-keys -t "$session_name" "zellij -f '$nested_config' attach" Enter
    zellij switch-client -t "$session_name"

    printf_green "âœ… Nested session created with single status bar"
  else
    # Not in zellij - create normal session that supports nesting
    printf_blue "ðŸš€ Creating zellij session with nested support: $session_name"
    zellij -f "$nested_config" new-session -D -A -s "$session_name" -c "$ZELLIJ_NEW_CWD"
  fi

  return $?
}
