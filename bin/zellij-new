# create the config file
__gen_config() {
  local NOTIFY_CLIENT_NAME="$APPNAME"
  if [ "$INIT_CONFIG" != "TRUE" ]; then
    printf_blue "Generating the config file in"
    printf_cyan "$ZELLIJ_NEW_CONFIG_DIR/$ZELLIJ_NEW_CONFIG_FILE"
  fi
  mkdir -p "$ZELLIJ_NEW_CONFIG_DIR" "$ZELLIJ_NEW_CONFIG_BACKUP_DIR" 2>/dev/null
  [ -f "$ZELLIJ_NEW_CONFIG_DIR/$ZELLIJ_NEW_CONFIG_FILE" ] &&
    cp -Rf "$ZELLIJ_NEW_CONFIG_DIR/$ZELLIJ_NEW_CONFIG_FILE" "$ZELLIJ_NEW_CONFIG_BACKUP_DIR/$ZELLIJ_NEW_CONFIG_FILE.$$" 2>/dev/null
  cat <<EOF >"$ZELLIJ_NEW_CONFIG_DIR/$ZELLIJ_NEW_CONFIG_FILE"
# Settings for zellij-new
ZELLIJ_NEW_CONFIG_DIR="${ZELLIJ_NEW_CONFIG_DIR:-}"
# - - - - - - - - - - - - - - - - - - - - - - - - -
# Project directory options
RUN_OPTIONS_DIRS_PROJECTS="${RUN_OPTIONS_DIRS_PROJECTS:-}"
# - - - - - - - - - - - - - - - - - - - - - - - - -
# Docker Project directory options
RUN_OPTIONS_DIRS_DOCKER_SRC="${RUN_OPTIONS_DIRS_DOCKER_SRC:-}"
# - - - - - - - - - - - - - - - - - - - - - - - - -
# Github Project directory options
RUN_OPTIONS_DIRS_GITHUB_DOCKERMGR="${RUN_OPTIONS_DIRS_GITHUB_DOCKERMGR:-}"
# - - - - - - - - - - - - - - - - - - - - - - - - -
# Programs logging Directories
RUN_OPTIONS_LOGS_BUILDX="${RUN_OPTIONS_LOGS_BUILDX:-}"
# - - - - - - - - - - - - - - - - - - - - - - - - -
# Color Settings
ZELLIJ_NEW_OUTPUT_COLOR_1="${ZELLIJ_NEW_OUTPUT_COLOR_1:-}"
ZELLIJ_NEW_OUTPUT_COLOR_2="${ZELLIJ_NEW_OUTPUT_COLOR_2:-}"
ZELLIJ_NEW_OUTPUT_COLOR_GOOD="${ZELLIJ_NEW_OUTPUT_COLOR_GOOD:-}"
ZELLIJ_NEW_OUTPUT_COLOR_ERROR="${ZELLIJ_NEW_OUTPUT_COLOR_ERROR:-}"
# - - - - - - - - - - - - - - - - - - - - - - - - -
# Notification Settings
ZELLIJ_NEW_NOTIFY_ENABLED="${ZELLIJ_NEW_NOTIFY_ENABLED:-}"
ZELLIJ_NEW_GOOD_NAME="${ZELLIJ_NEW_GOOD_NAME:-}"
ZELLIJ_NEW_ERROR_NAME="${ZELLIJ_NEW_ERROR_NAME:-}"
ZELLIJ_NEW_GOOD_MESSAGE="${ZELLIJ_NEW_GOOD_MESSAGE:-}"
ZELLIJ_NEW_ERROR_MESSAGE="${ZELLIJ_NEW_ERROR_MESSAGE:-}"
ZELLIJ_NEW_NOTIFY_CLIENT_NAME="${ZELLIJ_NEW_NOTIFY_CLIENT_NAME:-}"
ZELLIJ_NEW_NOTIFY_CLIENT_ICON="${ZELLIJ_NEW_NOTIFY_CLIENT_ICON:-}"
ZELLIJ_NEW_NOTIFY_CLIENT_URGENCY="${ZELLIJ_NEW_NOTIFY_CLIENT_URGENCY:-}"

EOF
  if builtin type -t __gen_config_local | grep -q 'function'; then __gen_config_local; fi
  if [ -f "$ZELLIJ_NEW_CONFIG_DIR/$ZELLIJ_NEW_CONFIG_FILE" ]; then
    [ "$INIT_CONFIG" = "TRUE" ] || printf_green "Your config file for $APPNAME has been created"
    . "$ZELLIJ_NEW_CONFIG_DIR/$ZELLIJ_NEW_CONFIG_FILE"
    exitCode=0
  else
    printf_red "Failed to create the config file"
    exitCode=1
  fi
  return ${exitCode:-0}
}
