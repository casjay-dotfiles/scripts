.SH TIPS
.SS Creating Custom Templates
.RS
.EX
# Copy default templates
gen-script --copy

# Edit templates
cd ~/.config/gen-script/templates/bash/
vim custom-script

# Use custom template
gen-script bash custom-script myapp.sh
.EE
.RE
.SS Batch Script Generation
.RS
.EX
# Generate multiple scripts
for script in app1 app2 app3; do
  gen-script bash script "$script.sh" --no
done

# Generate with metadata
gen-script bash script myapp.sh \\
  --desc "My application" \\
  --todo "Add error handling" \\
  --license MIT
.EE
.RE
.SS Header Management
.RS
.EX
# Replace existing header
gen-script bash script oldscript.sh --replace

# Import metadata from existing script
gen-script bash script newscript.sh --prev oldscript.sh

# Update header metadata
gen-script bash script script.sh --replace \\
  --desc "Updated description" \\
  --changelog "Added new feature"
.EE
.RE
